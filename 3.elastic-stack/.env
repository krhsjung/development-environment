
# This compose project name
COMPOSE_PROJECT_NAME=elastic-stack

# Password for the 'elastic' user (at least 6 characters)
ELASTIC_PASSWORD=${ELASTIC_PASSWORD}

# Password for the 'kibana_system' user (at least 6 characters)
KIBANA_PASSWORD=${KIBANA_PASSWORD}

# Version of Elastic products
STACK_VERSION=8.18.0

# Set the cluster name
CLUSTER_NAME=elastic-stack-cluster

# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic
#LICENSE=trial

# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9200
#ES_PORT=127.0.0.1:9200

# Port to expose Kibana to the host
KIBANA_PORT=5601

# Port to expose Logstash to the host
# LOGSTASH_PORT=6000

# Increase or decrease based on the available host memory
ES_MEM_LIMIT=4g
KB_MEM_LIMIT=1g
LS_MEM_LIMIT=1g
FB_MEM_LIMIT=1g

# SAMPLE Predefined Key only to be used in POC environments
# command: openssl rand -hex 32
KIBANA_XPACK_ENCRYPTION_KEY=${KIBANA_XPACK_ENCRYPTION_KEY}